<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffeeeff3" android:endColor="#ffeeeff3" android:angle="270.0" android:centerY="0.75" android:centerColor="#ffeeeff3" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <solid android:color="#ff3cffde" />
            </shape>
        </clip>
    </item>
</layer-list>